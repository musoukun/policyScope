{
  "$schema": "https://railway.com/railway.schema.json",
	"root": "mastra",
  "build": {
    "nixpacksPlan": {
      "phases": {
        "install": {
          "cmds": ["npm install --legacy-peer-deps"]
        },
        "build": {
          "cmds": ["npm run build"]
        }
      }
    }
  },
  "deploy": {
    "startCommand": "npm run start"
  }
}